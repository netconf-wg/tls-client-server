<rpc message-id="101"
  xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <generate-public-key
    xmlns="urn:ietf:params:xml:ns:yang:ietf-tls-common"
    xmlns:tlscsa="urn:ietf:params:xml:ns:yang:iana-tls-cipher-suite-algs">
    <algorithm>tlscsa:tls-ecdhe-psk-with-aes-128-gcm-sha256</algorithm>
    <bits>521</bits>
    <encrypt-with>
      <asymmetric-key-ref>hidden-asymmetric-key</asymmetric-key-ref>
    </encrypt-with>
  </generate-public-key>
</rpc>
